msgstr "Tidak ada tipe permasalahan yang sama pada koleksi yang dipilih."

#: common/lib/xmodule/xmodule/library_content_module.py
msgid "Select another problem type."
msgstr "Pilih tipe soal lain"

#: common/lib/xmodule/xmodule/library_content_module.py
#, python-brace-format
msgid "The specified library is configured to fetch {count} problem, "
msgid_plural "The specified library is configured to fetch {count} problems, "
msgstr[0] "Koleksi yang dipilih dikonfigurasi untuk mengambil {count} masalah,"
msgstr[1] "Koleksi yang dipilih dikonfigurasi untuk mengambil {count} masalah,"

#: common/lib/xmodule/xmodule/library_content_module.py
#, python-brace-format
